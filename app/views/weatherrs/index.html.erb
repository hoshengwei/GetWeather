<p id="notice"><%= notice %></p>

<h1>Weatherrs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weatherrs.each do |weatherr| %>
      <tr>
        <td><%= weatherr.date %></td>
        <td><%= link_to 'Show', weatherr %></td>
        <td><%= link_to 'Edit', edit_weatherr_path(weatherr) %></td>
        <td><%= link_to 'Destroy', weatherr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weatherr', new_weatherr_path %>
